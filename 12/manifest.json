{
    "manifest_version": 2,
    "name": "1,2 Number Game",
    "version": "1.1",
    "description": "An improved number game extension",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/iconbig.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/iconbig.png"
      }
    },
    "permissions": ["storage", "clipboardRead", "clipboardWrite"]
  }
  